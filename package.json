{
  "name": "paws-cli",
  "version": "0.0.4",
  "author": "Chris Guy @christien_guy",
  "bin": {
    "paws-cli": "./bin/run"
  },
  "bugs": "https://github.com/ChristienGuy/paws-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-update": "^1.3.2",
    "@types/mkdirp": "^0.5.2",
    "@types/mock-fs": "^3.6.30",
    "chalk": "^2.4.1",
    "mkdirp": "^0.5.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10.10.1",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "mock-fs": "^4.7.0",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ChristienGuy/paws-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "paws-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.trustedhousesitters.cli"
    }
  },
  "repository": "ChristienGuy/paws-cli",
  "scripts": {
    "pack": "oclif-dev pack",
    "pack:macos": "tsc && oclif-dev pack:macos",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
